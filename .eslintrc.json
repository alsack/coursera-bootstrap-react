{
    "extends": "google",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": ["error", {
            "arrays": "never",
            "objects": "never",
            "imports": "never",
            "exports": "never",
            "functions": "ignore"
        }],
        "max-len": ["error", {"code": 100}]
    }
}